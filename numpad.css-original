<pre style="word-wrap: break-word; white-space: pre-wrap;">/*
  1. Base

*/
html {width: 100%;height:100%;margin:0;padding:0;font-size:62.5%;}
body {width: 100%;height:100%;margin:0;padding:0;font-size:1.3rem;position:relative;line-height:1;font-family: 'Noto Sans KR', sans-serif, lato;color:#333;word-spacing:-1px;-webkit-overflow-scrolling: auto;}
* {box-sizing: border-box; outline: 0;-webkit-text-size-adjust: none;-webkit-tap-highlight-color: rgba(0, 0, 0, 0);}
*:after,
*:before {-webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;}

img {display:block;line-height:1;border:none;vertical-align: middle;}
cite, code, dfn, del, em, ins, label, q, span, strong {display:inline-block;}
cite, em, dfn {font-style:normal;}
sub {vertical-align:bottom;font-family:'dotum';}
sup {vertical-align:top;font-family:'dotum';}
hr {display:none;}
h1, h2, h3, h4, h5, h6 {margin:0;font-size:1em;}
address {font-style:normal;}
/^ p {margin:0;} ^/
pre {margin:0;font-size:1em;font-family:verdana;}
table {border-spacing:0;border-collapse:collapse;width:100%;}
caption {display:none;}
th,td {padding:0;text-align:left;font-weight:normal;font-size:inherit;color:inherit;vertical-align:middle;}
dd, dl, ol, ul {margin:0;padding:0;list-style-type:none;}
form {margin: 0;padding: 0;}
fieldset {margin:0;padding:0;border:none;}
legend {display:none;}
textarea {font-family:inherit;font-size:inherit;color:inherit;border:0.1rem solid #e2e2e2;border-radius:0;}
input [type="text"],
input.text {padding-left:0.5rem;height:3rem;line-height:1.4;font-family:inherit;font-size:inherit;color:#333;background:#fff;vertical-align:middle;border:0.1rem solid #e3e3e3;border-radius:0;-webkit-appearance: none; -moz-appearance: none;}
input[type=text]::-ms-clear {display: none;}
select {margin:0;padding:0 1.5rem 0 0.5rem;height:3rem;font-family:inherit;font-size:inherit;line-height:1.4;color:#333;vertical-align:middle;border:0.1rem solid #e3e3e3;border-radius:0;background-color:#fff;background-image:url(../images/common/bg_select.png);background-repeat:no-repeat;background-position: right 0.5rem center;background-size: auto 22%;-webkit-appearance: none; -moz-appearance: none;}
select::-ms-expand {display: none;}
input[readonly],
input[type="text"]:disabled,
select:disabled {background-color:#ededed;}
input[readonly].hasDatepicker {background-color: #fff;}
input.text.c {padding-left:0;}

button {display:inline-block;margin:0;padding:0;font-family:inherit;white-space:nowrap;text-align: center;vertical-align: middle;cursor:pointer;-webkit-appearance: none; -moz-appearance: none;}
a {color:inherit; text-decoration:none;}
a.btn:focus, input.btn:focus,button.btn:focus {outline: none;}
::-webkit-input-placeholder {color: #c1b9b9;}
:-moz-placeholder {color: #c1b9b9;}
::-moz-placeholder {color: #c1b9b9;}
:-ms-input-placeholder {color: #c1b9b9 !important;}
::-ms-input-placeholder {color: #c1b9b9;}
::placeholder {color: #c1b9b9;}
/*
  2. Components
*/
div.control-container {
  margin-top: 10px;
  padding-bottom: 10px; }
  div.control-container div.amplitude-play-pause {
    width: 74px;
    height: 74px;
    cursor: pointer;
    float: left;
    margin-left: 10px; }
  div.control-container div.amplitude-play-pause.amplitude-paused {
    background: url("../img/play.svg");
    background-size: cover; }
  div.control-container div.amplitude-play-pause.amplitude-playing {
    background: url("../img/pause.svg");
    background-size: cover; }
  div.control-container div.meta-container {
    float: left;
    width: calc(100% - 84px);
    text-align: center;
    color: white;
    margin-top:0.9rem; }
    div.control-container div.meta-container span[data-amplitude-song-info="name"] {
      font-size:2rem;
      color: #4765e8;
      display: block; text-align: left; margin-left:5%; font-weight: 600}
    div.control-container div.meta-container span[data-amplitude-song-info="artist"] {
      font-weight: 100;
      font-size: 14px;
     text-align: left;
     margin-left: 10px;
     margin-right: 10px;
      color: #fff;
      display: block; }
  div.control-container:after {
    content: "";
    display: table;
    clear: both; }

/*
  Small only
*/
@media screen and (max-width: 39.9375em) {
  div.control-container div.amplitude-play-pause {
    background-size: cover;
    width:5rem;
    height:5rem; }
  div.control-container div.meta-container {
    width: calc(100% - 74px); } }
/*
  Medium only
